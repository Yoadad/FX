<script type="text/html" id="infoMessageTemplate">

    <div class="alert alert-{{=it.type}} alert-dismissible fade in" role="alert"> 
        <button type="button" class="close" data-dismiss="alert" aria-label="Close">
            <span aria-hidden="true">×</span>
        </button> 
        <strong>Message:</strong> {{=it.message}} 
    </div>

</script>
